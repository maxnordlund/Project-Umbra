<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project Umbra">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="dist/Umbra.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="se.kth.csc.umbra.Main"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin"/>
        	<fileset dir="img"></fileset>
        </jar>
    </target>
</project>
